{
  "name": "picgo-plugin-custom-path",
  "version": "1.0.7",
  "description": "A PicGo plugin for customizing upload path",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/melon95/picgo-plugin-custom-path",
  "repository": {
    "type": "git",
    "url": "https://github.com/melon95/picgo-plugin-custom-path.git"
  },
  "scripts": {
    "build": "tsc -p .",
    "dev": "tsc -w -p .",
    "prepare": "husky install"
  },
  "keywords": [
    "picgo",
    "picgo-gui-plugin",
    "picgo-plugin",
    "picgo filename",
    "picgo custom"
  ],
  "author": "melon95",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "latest",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "picgo": "^1.2.0",
    "prettier": "^2.7.1",
    "prettier-config-standard": "^5.0.0",
    "typescript": "^4.7.4"
  },
  "lint-staged": {
    "src/**/*.ts?(x)": [
      "eslint",
      "prettier --write",
      "git add"
    ]
  }
